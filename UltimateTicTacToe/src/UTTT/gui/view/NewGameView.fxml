<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" prefHeight="102.0" prefWidth="78.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UTTT.gui.MainController.NewGameController">
    <children>
        <GridPane prefHeight="102.0" prefWidth="79.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="btnPvP" mnemonicParsing="false" onAction="#handlePlayerVersusPlayer" text="PvP" />
                <Button mnemonicParsing="false" onAction="#handlePlayerVersusAI" text="PvAI" GridPane.rowIndex="1" />
                <Button mnemonicParsing="false" onAction="#handleAIVersusAI" text="AIvAI" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
