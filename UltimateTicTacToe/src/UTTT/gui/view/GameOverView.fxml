<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UTTT.gui.MainController.GameOverController">
    <top>
        <Label text="Game Over" BorderPane.alignment="CENTER">
            <padding>
                <Insets left="10.0" right="10.0" top="10.0" />
            </padding>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <font>
                <Font size="36.0" />
            </font>
        </Label>
    </top>
    <center>
        <Label fx:id="lblMessage" text="ended up as a DRAW!" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </center>
    <bottom>
        <AnchorPane prefHeight="30.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#handleQuit" text="Quit" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="0.0" />
                <Button mnemonicParsing="false" onAction="#handleNewGame" text="New Game" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="50.0" />
            </children>
        </AnchorPane>
    </bottom>
</BorderPane>
